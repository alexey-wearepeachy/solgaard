/*============================================================================
  #Product Page
==============================================================================*/
.template-product {
  .main-content{
    padding-bottom: 0;
  }

  .product-section > .wrapper:last-of-type{
    padding-bottom: 120px;
  }

  .shopify-product-form + .banana-container {
    display: none;
  }
}

.product-page {
  padding-top: $gutter*2;
  padding-bottom: $gutter*2;
  background-color: $colorProductBg;
}

.product__slides-wrap[data-thumbs-position="side"] {
  position: relative;

  @include at-query ($min, $large) {
    .product__slides {
      margin-left: 80px;
      margin-bottom: 10px;
    }

    .product__thumbs {
      position: absolute;
      width: 80px;
      top: 0;
      right: auto;
      bottom: auto;
      left: 0;
    }
  }
}

.product__slides {
  margin-bottom: $gutter;
  overflow-y: hidden;

  a,
  img {
    display: block;
    margin: 0 auto;
  }

  li {
    margin-bottom: $gutter;
  }

  .slick-prev {
    left: 10px;
  }

  .slick-next {
    right: 10px;
  }
}

.product__zoom,
.product__popup-toggle {
  margin-bottom: $gutter;
  font-size: .9em;
  text-decoration: none;

  > span {
    display: inline-block;
    vertical-align: middle;
  }

  span + span {
    // margin-left: 5px;
  }

  svg {
    display: block;
  }
}


.product__zoom {
  @include at-query ($min, $large) {
    margin-left: 80px;
  }

  span + span {
    margin-left: 10px;
  }
}

.product__popup-toggle {
  display: block;
  font-size: 1em;
  cursor: pointer;
  margin-bottom: $gutter/2;

  @media only screen and (min-width: 375px) {
    margin-bottom: $gutter;
    float: right;
  }

  @include at-query ($min, $large) {
    margin-bottom: $gutter/2;
    float: none;
  }

  @media only screen and (min-width: 940px) {
    margin-bottom: $gutter;
    float: right;
  }

  .product__popup-toggle__text {
    margin-bottom: 0;
  }
}

.product__thumbs {
  .slick-dots {
    position: relative;

    li {
      height: 60px;
      width: 60px;
      border-radius: 8px;
      border: 2px solid #f7f7f7;
      background-color: #f7f7f7;
      opacity: .7;
      overflow: hidden;

      @include at-query ($max, $small+1) {
        height: 35px;
        width: 35px;
      }
    }

    .slick-active {
      border: 2px solid #000;
      opacity: 1;
    }

    button {
      background-color: #fafafa;
    }
  }
}

// video button
.product__thumbs__video {
  display: block;
  position: relative;

  .btn--play {
    @include transform(translateX(-50%)translateY(-50%));
    position: absolute;
    top: 50%;
    right: auto;
    bottom: auto;
    left: 50%;
    width: 30px;
    height: 30px;
    border: 0;
  }

  &:after {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
  }
}

.product__photo {
  position: relative;

  .image-wrapper {
    // background: {
    //   image: url({{ 'loader.gif' | asset_url }});
    //   size: 30px;
    //   repeat: no-repeat;
    //   position: center;
    // }
  }
}

.product__photo--video {
  > iframe + * {
    display: none;
  }
}

.product__play {
  position: absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
  opacity: 1;
  @include transition(opacity 0.2s ease-out);

  .btn--play {
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -50px;
    margin-left: -50px;
    width: 100px;
    height: 100px;
    border: 0;
  }

  &:hover{
    opacity: 0.75;
  }
}

.product__slides .product__photo--additional{ display: none; }
.product__slides.slick-initialized .product__photo--additional{ display: block; }

.product__title {
  margin-bottom: 0;
  color: $colorBlack;
}

.product__review-header {
  min-height: 20px;
  margin: .25em 0;

  .product__review-star-rating,
  .product__review-question {
    display: inline-block;
  }

  .product__review-star-rating {
    margin-right: 30px;
  }

  .product__review-question {
    display: none;
  }
}


.product__price {
  margin: .75em 0;
  font-weight: bold;
  color: $colorBlack;
  letter-spacing: normal;

  &.on-sale .compare-at + .money {
    color: $colorRed;
  }

  @include at-query ($min, $large) {
    &.on-sale .compare-at {
      display: inline-block;
      margin: 0 .25em 0;
      // font-size: .8em;
    }
  }
}

.add-to-cart-wrapper {
  margin-top: $gutter/2;

  button[type="submit"] {
    border: 2px solid $colorTeal;
  }
}

.product__preorder-message {
  display: none;

  .variant--preorder & {
    display: block;
  }

  p {
    margin: .25em 0 1em;
    font-size: 1em;
    text-align: center;
  }
}

.product__description {
  margin-top: $gutter/2;
  margin-bottom: $gutter;
  color: $colorBlack;

  p {
    font-size: 1.1em;
  }
}

.product__benefits {
  margin: 1em 0 0;

  .product__benefits__item {
    display: block;
    width: 33.33%;
    margin: 0;
    float: left;
    text-align: center;
  }

  .product__benefits__icon,
  .product__benefits__text {
    display: block;
    font-size: .9em;
    vertical-align: middle;
    color: $colorBlack;
    text-transform: uppercase;
    opacity: 1;
  }

  .product__benefits__icon {
    width: 75px;
    height: 75px;
    margin: 0 auto;
  }

  &:after {
    display: block;
    content: "";
    clear: both;
  }
}

.product__arrows {
  padding-bottom: $gutter;
}

.product__related {
  margin-bottom: $gutter*2;
}

.selector-wrapper select,
.product-form__variants{
  margin: 0 auto 2em auto;;
  border-left: none;
  border-right: none;
  border-top: none;
  border-radius: 0px;
  text-align-last:center;
  padding-left: 28px;
}

.product-form__variants {
  display: none;
  .no-js  & {
    display: block;
  }
}


.product-form label,
.selector-wrapper label,
.quantity-selector__wrapper label{
  margin-top: 1em;
  font-size: 0.93721rem;
  font-weight: bold;
  letter-spacing: 1px;
  color: $colorTextBody;
  @extend .title;
}

.product__wait-list {
  .mc-field-group {
    margin-bottom: 1em;
    label {
      display: none;
    }

    input {
      margin-bottom: 0;
    }
  }

  .response {
    position: relative;
    padding: .75rem 1.25rem;
    margin-bottom: 1rem;
    border: 1px solid transparent;
    border-radius: .25rem;
  }

  #mce-error-response {
    color: #721c24;
    background-color: #f8d7da;
    border-color: #f5c6cb;
  }

  #mce-success-response {
    color: #155724;
    background-color: #d4edda;
    border-color: #c3e6cb;
  }
}

.product__inventory-message {
  font-size: 1em;
}

.product__banana-stand {
  margin-top: 15px;
  font-size: .88em;
  text-align: center;
  opacity: .8;

  .banana-socialProofFeed-container {
    display: none;
  }

  > * {
    display: inline-block;
    font-size: .9em;
  }

  > .product__inventory-message {
    font-size: 1em;
    color: $colorRed;
  }
}

.wait-list__wrapper {
  display: none;
}

.variant--soldout,
.variant--unavailabe {
  .quantity-selector__wrapper {
    display: none;
  }

  .add-to-cart-wrapper {
  }

  .quantity-selector__wrapper {
    opacity: 0;

    label, input, button{
      cursor: default;
    }
  }
}

.klaviyo-bis-trigger {
  display: none;
}

.variant--soldout{
  .wait-list__wrapper {
    display: block !important;
  }
  .klaviyo-bis-trigger {
    display: block !important;
  }
}

.quantity-selector__wrapper{
  transition: opacity 0.4s ease-out;

  .js-qty {
    margin: 0 !important;
  }
}

.share{
  padding-bottom: $gutter;
  padding-top: $gutter;
}

.share__link{
  text-decoration: none;
  margin-left: $gutter/2;
  margin-right: $gutter/2;
}

.border-bottom-link {
  text-decoration: none;
  cursor: pointer;
  border-bottom: 1px solid $colorBorder;
  padding-bottom: 3px;
  &:hover{
    border-bottom: 1px solid darken($colorBorder, 10%);
  }
}

.product-description--below {
  margin-top: $gutter*2;
}

.mfp-content .modal--compare {
  width: 800px;
  max-width: 100%;

  .compare__heading {
    margin-bottom: 0;
    color: $colorBlack;
  }

  .compare__body {
    text-align: center;
    background-color: #fcfcfe;
  }

  .compare__img {
    width: 300px;
    max-width: 100%;
    margin-right: auto;
    margin-left: auto;
  }

  .compare__variant-title {
    color: $colorBlack;
  }

  .compare__variant-price {

  }

  .compare__features {
    display: inline-block;

    > li:before {
      content: "";
      margin-right: 5px;
    }
  }
}


.okeReviews .okeReviews-starRating-indicator-layer--foreground {
  background-image: url({{ 'star-rating-foreground.svg' | asset_url }}) !important;
}

.product__reviews-widget {
  overflow: hidden;

  .okeReviews .okeReviews-reviewsWidget.is-okeReviews-reviewsWidget-large {
    .okeReviews-review-primary,
    .okeReviews-review-side,
    .okeReviews-reviewsAggregate-primary,
    .okeReviews-reviewsAggregate-side {
      border: 0;
      border-top: 1px solid #eee;
    }

    .okeReviews-reviewsAggregate-side {
      padding-left: 0;
    }
  }

  .okeReviews .okeReviews-reviews-footer::before {
    display: none;
  }

  .okeReviews .okeReviews-review-main-heading {
    margin-bottom: 1em;
    font-size: 1.4em;
  }

  .okeReviews p {
    font-size: 1em;
    line-height: 1.4;
  }
}

.product__out-of-stock-message p {
  font-size: .9em;
  color: $colorRed;
  text-align: center;
  text-transform: uppercase;
}

/*============================================================================
  #Blogs and Comments
==============================================================================*/

.section-blog,
.section-recent-articles {
  background-color: #fbfbfc;
}

.article-grid-item {
  margin-bottom: 30px;

  .grid__body {
    position: relative;
    border-radius: 3px;
    box-shadow: 0 0 10px rgba(200, 200, 200, .4);
    overflow: hidden;
    background-color: $colorWhite;
  }

  .grid__header {
    position: relative;
    padding-bottom: 100%;
    overflow: hidden;
  }

  .grid__image {
    @include transition( .1s transform ease-in-out );
    @include transform( scale(1) );
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: {
      size: cover;
      color: #ccc;
      position: center;
      repeat: no-repeat;
    }
  }

  &:hover .grid__image {
    @include transform( scale(1.05) );
  }

  .grid__tags {
    margin-bottom: $gutter/4;
  }

  .grid__date {
    display: block;
    margin-bottom: $gutter/3;
    text-transform: uppercase;
    font-size: .9em;
    font-weight: 600;
  }

  .grid__meta {
    padding: $gutter/2;
  }

  .grid__author {
    margin: $gutter/2 0 0;
    font-size: .9em;
    font-weight: 600;
  }

  .grid__link {
    text-decoration: none;
  }
}

.article-grid-item--blog {
  .grid__date {
    display: none;
  }
}

.article-grid-item--press,
.article-grid-item--latest-coverage {
  .grid__author {
    display: none;
  }
}

.article-grid-item--latest-coverage {
  .grid__header {
    padding-bottom: 50%;
  }
}

.article-grid-item--press {
  .grid__body {
    @include clearfix();

    @include at-query ($min, $small+1) {
      > .grid__link {
        display: block;
        width: 40%;
        float: left;
      }

      > .grid__meta {
        @include transform( translateY(-50%) );
        position: absolute;
        width: 60%;
        float: none;
        left: 40%;
        top: 50%;
        padding-right: $gutter*1.5;
        padding-left: $gutter*1.5;
      }
    }

    @media only screen and (max-width: 768px) and (min-width: 481px) {
      > .grid__meta {
        padding-right: $gutter;
        padding-left: $gutter;
      }

      .grid__title {
        font-size: 1.1em;
      }

      .grid__date {
        font-size: .8em;
      }
    }

    @include at-query ($min, $large) {
      > .grid__link {
        width: 25%;
      }

      > .grid__meta {
        width: 75%;
        left: 25%;
      }
    }

    .grid__date {
      margin-bottom: 0;
    }

    .equalHeightColumn {
      height: auto !important;
    }
  }

  .grid__tags {
    pointer-events: none;

    .article__tag:before {
      content: "";
    }
  }
}

.article-grid-item--our-team {
  .grid__tags,
  .grid__author {
    display: none;
  }
}

.article {
  padding: $gutter $gutter 0;

  @include at-query ($min, $large) {
  padding: $gutter*2 $gutter*2 0;
  }
}

.article--over-image {
  border: none;
  margin-top: -140px;
  background-color: $colorWhite;
  z-index: 100;
  position: relative;

  @include at-query ($max, $medium) {
    margin-top: -100px;
  }
}

.article--our-team {
  .article__tags,
  .article__author {
    display: none;
  }

}

.article__header {
  margin-bottom: $gutter;

  .article__author {
    font-size: 14px;
  }

  .article__author__image,
  .article__author__meta {
    display: inline-block;
    vertical-align: middle;
  }

  .article__author__image {
    width: 30px;
    height: 30px;
    border-radius: 50%;
    background: {
      size: cover;
      color: #fafafa;
    }

    + .article__author__meta {
      margin-left: 5px;
    }
  }

  .article__author__image--placeholder {
    background: {
      position: -2px 50%;
      size: 200px;
      repeat: no-repeat;
      image: url({{ 'logo-solgaard-design.png' | file_img_url: '300x' }});
      size: contain;
      image: url({{ 'nav-icon-team-hex.png' | file_img_url: '300x' }});
      repeat: no-repeat;
    }
  }
}

.article__footer {
  margin-bottom: $gutter;
}

.article__tags {
  margin-bottom: $gutter;
}

.article__tag {
  @include transition( .2s background-color ease-in-out );
  display: inline-block;
  position: relative;
  margin-bottom: 3px;
  padding: 2px 10px;
  font-size: .9em;
  font-weight: bold;
  text-transform: uppercase;
  text-decoration: none;
  background-color: $colorBlack;
  color: $colorWhite;

  &:active,
  &:focus,
  &:hover {
    background-color: lighten( $colorBlack, 5%);
    color: $colorWhite;
  }

  &:before {
    content: '#';
  }
}

.article__content {
  line-height: 2.22;

  .rte__image-wrapper,
  .rte__video-wrapper {
    @include at-query ($min, $large) {
      width: 130%;
      max-width: 130%;
      margin-left: -15%;
    }
  }

  .rte__image-wrapper img {
    @include at-query ($min, $large) {
      width: 100%;
    }
  }

 .rte__video-wrapper {
    @include at-query ($min, $large) {
      padding-bottom: 73.099%;
    }
  }
}

.article__newsletter {
  .section-newsletter {
    margin: $gutter 0;
    padding: 0;
    background-color: $colorWhite;

    .section-body {
      background-color: #fbfbfc;
    }

    .wrapper {
      width: 100%;
      padding: 0;
    }

    .grid__item {
      width: 100%;
      left: 0;
    }
  }
}

.blog__header + .blog__items .section-body {
  padding-top: 0;
}

.blog__title {
  @include at-query ($min, $large) {
    font-size: 4.6em;
    margin-bottom: 0;
  }
}

.blog__description {
  font-weight: 600;
}

.blog__featured-article > .article-grid-item {
  width: 100%;

  @include at-query ($min, $large) {
    .grid__body {
      box-shadow: none;
      background-color: transparent;
    }

    .grid__header {
      width: 70%;
      padding-bottom: 50%;
    }

    .grid__meta {
      @include transform( translateY(-50%) );
      position: absolute;
      width: 40%;
      height: auto !important;
      top: 50%;
      right: 0;
      padding: $gutter*2 $gutter;
      border-radius: 3px;
      box-shadow: 0 0 10px rgba(200, 200, 200, .4);
      background: $colorWhite;
    }

    .grid__title {
      font-size: 2.4em;
    }
  }
}

/*============================================================================
  #Cart Page
==============================================================================*/
.cart__row {
  position: relative;
  margin-top: $gutter;
  padding-top: $gutter;
  border-top: 1px solid $colorBorder;

  &:first-child {
    margin-top: 0;
  }

  &:first-child {
    padding-top: 0;
    border-top: 0 none;
  }
  a{
    text-decoration: none;
  }
  .js-qty {
    margin: 0;
  }


  &[data-free-solar-sling] {
    .js-qty,
    .ajaxcart__qty {
      visibility: hidden;
    }

    a {
      pointer-events: none;
    }

    .cart__remove {
      pointer-events: auto;
    }
  }
}

@include at-query ($min, $large) {
  .cart__row--table-large {
    display: table;
    table-layout: fixed;
    width: 100%;

    .grid__item {
      display: table-cell;
      vertical-align: middle;
      float: none;
    }
  }
}
.cart__image {
  display: block;

  img {
    display: block;
  }
}
.cart__subtotal {
  margin: 0 0 0 ($gutter / 3);
  display: inline;
}
.cart__mini-labels {
  display: block;
  margin: ($gutter / 3) 0;
  font-size: em(12px * $bodyFontAdjust);

  @include at-query ($min, $large) {
    display: none;
  }
}
.cart__remove {
  display: block;
}
.cart__footer{
  text-align: right;
}
.cart__instructions__label{
  text-align: left;
}
.hidden-count{
  display: none;
}

.additional-checkout-buttons {
  margin: 20px 0;
  padding: 8px;
  background-color: darken($colorBody, 2%);
  border: 1px solid darken($colorBody, 5%);
  text-align: right;

  & > *:not(script) {
    padding: 12px 0 0 5px;
    vertical-align: top;
    line-height: 1;

    &:first-child,
    &:empty {
      padding-left: 0px;
    }
  }
  @include at-query ($min, $small) {
    padding: 18px 18px 8px 18px;
  }
}



.ajaxcart .additional-checkout-buttons{
  margin: 0;
  padding: 0;
  background-color: darken-smart($colorSplash, 20%);
  border: none;
  text-align: center;
  min-height: 0;
  & > *:not(script) {
    margin-top: 8px;
    margin-bottom: 8px;
  }
}


/*============================================================================
  #Quantity Selectors
==============================================================================*/
.ajaxcart--hide{
  opacity: 0;
}

.js-qty {
  position: relative;
  margin-bottom: 1em;
  max-width: 103px;
  min-width: 75px;
  overflow: visible;
  @include backface(hidden);

  input[type="text"] {
    display: block;
    background: none;
    text-align: center;
    width: 100%;
    padding: 5px 30px;
    margin: 0;
  }
}

.product-single .js-qty{
  margin-left: auto;
  margin-right: auto;
}

.js-qty__adjust {
  cursor: pointer;
  position: absolute;
  display: block;
  padding: 0 12px;
  top: 0px;
  bottom: 0px;
  border: 0 none;
  background: none;
  text-align: center;
  overflow: hidden;
  -webkit-user-select: none;
  -moz-user-select:    none;
  -ms-user-select:     none;
  -o-user-select:      none;
  user-select:         none;
  @include transition(all 0.2s ease-out);

  &:hover,
  &:focus {
    color: $colorSplash;
    outline: none;
  }

  &:active {
    color: darken($colorSplash, 20%);
    outline: none;
  }
}

.update-cart:focus {
  outline: none;
}

.js-qty__adjust--plus {
  right: 0;
}

.js-qty__adjust--minus {
  left: 0;
}

