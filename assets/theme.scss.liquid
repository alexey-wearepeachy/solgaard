}

@keyframes contentAnimation {
  0% {
    transform: translateX(-50%) rotate(0deg);
  }
  100% {
    transform: translateX(-50%) rotate(-360deg);
  }
}

#animaa .containeraa .borderaa .contentaa,
#animaa .borderaa .borderaa .contentaa {
	width: 150%;
	height: 100%;
	background: white;
	position: absolute;
	top: 50%;
	left: 50%;
	animation: contentAnimation 2s infinite reverse linear;
