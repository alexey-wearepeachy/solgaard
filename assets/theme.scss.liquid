            width: 100%;
            overflow: hidden;
            z-index: 1;

            video {
              position: relative;
              top: 50%;
              left: 50%;
              transform: translateX(-50%) translateY(-50%);
              min-height: 100%;
              min-width: 100%;
              height: auto;
              width: auto;
              max-width: none;
              object-fit: cover;
              z-index: inherit;
