          }
          .s-hidden {
            display: none;
          }
          .styledSelect {
            display: flex;
            cursor: pointer;
            border-radius: 2px;
            padding: 12px 60px 12px 5px;
            align-items: center;
            position: relative;
            &:after {
              content: '';
              position: absolute;
              width: 11px;
              height: 2px;
              background: #000;
              top: 50%;
              right: 32px;
              transform: translateY(-50%) rotate(-45deg);
              transition: transform .5s ease;
            }
            &:before {
              content: '';
              position: absolute;
              width: 11px;
              height: 2px;
              background: #000;
              top: 50%;
              right: 38px;
              transition: transform .5s ease;
              transform: translateY(-50%) rotate(45deg);
            }
            &.opened {
              &:after {
                transform: translateY(-50%) rotate(-135deg);
              }
              &:before {
                transform: translateY(-50%) rotate(135deg);
              }
            }
            &.notclickable {
              pointer-events: none;
              &:after, &:before {
                content: none;
              }
