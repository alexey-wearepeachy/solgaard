#animaa .borderaa {
	width: 100%;
	height: 100%;
	background: #000;
	position: absolute;
	top: 50%;
	left: 50%;
	overflow: hidden;
	transform: translate(-50%, -50%);
	clip-path: polygon(0% 50%, 25% 95%, 75% 95%, 100% 50%, 75% 5%, 25% 5%);
}

#animaa .containeraa .borderaa,
#animaa .borderaa .borderaa {
	width: 90%;
