.modal {
  position: relative;

  &:after {
    display: block;
    content: "";
    clear: both;
  }
}

.modal__body {
  padding: $gutter/2 $gutter;

  @include at-query ($min, $large) {
    padding: $gutter*2 $gutter;
  }
}

.modal__image {
  padding-bottom: 50%;
  background-color: #ccc;
  background-position: center;
  background-size: cover;

  @include at-query ($min, $small) {
    width: 100%;
    height: 100%;
    padding-bottom: 0;
  }
}

.modal__heading,
.modal__subheading,
.modal__content,
.modal__form,
.modal__small-text {
  width: 300px;
  max-width: 100%;
}

.modal__heading,
.modal__subheading,
.modal__content {
  color: #000;

  .inverse & {
    color: #fff;
  }
}

.modal__heading {
  margin: 0;
  font-family: $bodyFontStack;
  font-size: 3em;
  line-height: 1;

