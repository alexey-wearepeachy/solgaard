  background-position: 0 0;
}

@keyframes icon-rotate {
  0% {
    @include transform(rotateY(0deg));
  }

  50% {
    @include transform(rotateY(180deg));
  }

  100% {
    @include transform(rotateY(360deg));
  }
}

.instagram-block--1 .instagram-block__bg { background-position: 66.66% 0; }
.instagram-block--2 .instagram-block__bg { background-position: 100% 0; }
.instagram-block--3 .instagram-block__bg { background-position: 66.66% 50%; }
.instagram-block--4 .instagram-block__bg { background-position: 100% 50%; }
.instagram-block--5 .instagram-block__bg { background-position: 0 100%; }
.instagram-block--6 .instagram-block__bg { background-position: 33.33% 100%; }
.instagram-block--7 .instagram-block__bg { background-position: 66.66% 100%; }
.instagram-block--8 .instagram-block__bg { background-position: 100% 100%; }
