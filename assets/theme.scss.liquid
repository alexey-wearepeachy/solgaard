            padding: 25px 24px 0 0;

            .projects-main-slider__wrapper {
              .projects-main-slider__slide {
                padding: 30px;
                min-height: 170px;
                max-height: 170px;

                .slide__img {
                  img {
                    max-width: 190px;
                    max-height: 120px;
                  }
                }
              }
            }
          }
        }

        .impact-projects__main-third-slider {
          margin: 50px 0 0;

          .main-slider__title {
            padding: 0 25px 0 0;
            font-size: 20px;
          }

          .projects-main-third-slider {
            padding: 25px 24px 0 0;

            .projects-main-slider__wrapper {
              .projects-main-slider__slide {
                padding: 30px;
                min-height: 170px;
                max-height: 170px;
              }
            }
          }
        }
      }
    }
  }
}
// ================================== IMPACT PROJECTS SECTION END =========================== //

// ================================== IMPACT HIGHLIGHTS CHAIN SECTION =========================== //
#impact-highlights {
  background-color: #334155;
  padding: 130px 0 0;
  overflow: hidden;

  @media (max-width: 768px) {
    padding: 60px 0 0;
  }

  .impact-highlights__container {
    max-width: 1650px;
    width: 100%;
    margin: 0 auto;
  }

  .impact-highlights__inner {
    display: flex;

    .impact-highlights__icon-block {
      position: relative;
      flex-shrink: 0;
      margin: 0 84px 0 157px;
      width: fit-content;

      @media (max-width: 1280px) {
        margin: 0 80px;
      }

      @media (max-width: 1050px) {
        margin: 0 40px;
      }

      @media (max-width: 950px) {
        display: none;
      }

      &:after {
        content: '';
        position: absolute;
        top: 0;
        left: 50%;
        transform: translate(50%, -100%);
        height: 100%;
        width: 2px;
        background-image: url("{{ 'dashed-line-big.svg' | asset_url }}");
        background-size: cover;
        background-repeat: no-repeat;
        background-position: top;
      }

      .impact-highlights__icon {
        display: flex;
        justify-content: center;
        align-items: center;
        flex-shrink: 0;
        width: 111px;
        height: 109px;
        margin: 0;
        border-radius: 100%;
        background-repeat: no-repeat;
        background-size: contain;
        background-position: center;

        img {
          display: block;
          width: 65px;
          height: 65px;
          object-fit: cover;
        }
      }
    }

    .impact-highlights__main-block {
      flex-grow: 1;
      padding: 28px 0 130px;

      @media (max-width: 950px) {
        width: 100%;
        padding: 28px 0 90px 40px;
      }

      .impact-highlights__icon-mob {
        display: none;
        justify-content: center;
        align-items: center;
        flex-shrink: 0;
        width: 115px;
        height: 115px;
        margin: 0 0 15px;
        border-radius: 100%;
        background-repeat: no-repeat;
        background-size: contain;
        background-position: center;

        img {
          display: block;
          width: 67px;
          height: 67px;
          object-fit: cover;
        }

        @media (max-width: 950px) {
          display: flex;
        }
      }

      .impact-highlights__main-title {
        padding: 0 40px 0 0;
        margin: 0 0 20px;
        font-family: 'Harmonia Sans', sans-serif;
        font-style: normal;
        font-weight: 600;
        font-size: 43px;
        line-height: 150%;
        color: #FFFFFF;
      }

      .impact-highlights__main-text {
        margin: 0;
        max-width: 835px;
        padding: 0 40px 0 0;
        font-family: 'Harmonia Sans', sans-serif;
        font-style: normal;
        font-weight: 400;
        font-size: 21px;
        line-height: 180%;
        color: #FFFFFF;
      }

      .impact-highlights__main-statistic-blocks {
        margin: 60px 0 0;
        padding: 0 40px 0 0;
        max-width: 875px;
        width: 100%;
        display: flex;
        align-items: stretch;
        justify-content: flex-start;
        flex-wrap: wrap;

        @media (max-width: 950px) {
          justify-content: space-between;
        }

        .statistic-block {
          width: calc(50% - 36px);
          padding: 35px 63px 58px 51px;
          background-color: #F9F9F9;
          margin: 0 36px 36px 0;
          transition: opacity .6s ease-in-out;

          &._impact-anim-items {
            opacity: 0;
          }

          &._impact-anim-active {
            opacity: 1;
          }

          @media (max-width: 1140px) {
            padding: 35px 40px 58px 45px;
          }

          @media (max-width: 950px) {
            margin: 0 0 36px 0;
            width: 48%;
          }

          @media (max-width: 740px) {
            width: 100%;
          }

          .statistic-block__title {
            margin: 0 0 9px;
            font-family: 'Harmonia Sans', sans-serif;
            font-style: normal;
            font-weight: 700;
            font-size: 19px;
            line-height: 150%;
            letter-spacing: 0.05em;
          }

          .statistic-block__info {
            display: flex;
            flex-direction: column;
            align-items: flex-start;

            .info-number {
              margin: 0 0 19px 0;
              font-family: 'Montserrat', sans-serif;
              font-style: normal;
              font-weight: 500;
              font-size: 60px;
              line-height: 60px;
              color: #000000;
            }

            .info-text {
              margin: 0 0 8px;
              font-family: 'Harmonia Sans', sans-serif;
              font-style: normal;
              font-weight: 400;
              font-size: 23px;
              line-height: 28px;
              color: #000000;
            }
          }
        }

        .statistic-blocks__capture {
          width: 100%;
          margin: 9px 0 0;
          font-family: 'Montserrat', sans-serif;
          font-style: normal;
          font-weight: 400;
          font-size: 16px;
          line-height: 170%;
          color: rgba(255, 255, 255, 0.7);

          &.lighter {
            font-weight: lighter;
          }

          &.normal {
            font-weight: normal;
          }

          &.bold {
            font-weight: bold;
          }
        }
      }

      @media (max-width: 768px) {
        padding: 0 0 100px 50px;

        .impact-highlights__main-title {
          font-size: 45px;
          margin: 0 0 34px;
          padding: 0 35px 0 0;
        }

        .impact-highlights__main-text {
          font-size: 22px;
          padding: 0 35px 0 0;
        }

        .impact-highlights__main-statistic-blocks {
          margin: 54px 0 0;
          padding: 0 50px 0 0;

          .statistic-block {
            margin: 0 0 28px;
          }

          .statistic-blocks__capture {
            margin: 0;
          }
        }
      }

      @media (max-width: 650px) {
        .impact-highlights__main-statistic-blocks {
          .statistic-block {
            margin: 0 0 28px;
            padding: 37px 67px 60px 54px;

            .statistic-block__title {
              margin: 0 0 14px;
            }

            .statistic-block__info {
              .info-number {
                margin: 0 0 15px 0;
                font-size: 64px;
                line-height: 64px;
              }
            }
          }
        }
      }

      @media (max-width: 450px) {
        padding: 0 0 100px 25px;

        .impact-highlights__main-title {
          font-size: 40px;
          margin: 0 0 16px;
          padding: 0 25px 0 0;
        }

        .impact-highlights__main-text {
          font-size: 16px;
          padding: 0 25px 0 0;
        }

        .impact-highlights__main-statistic-blocks {
          padding: 0 25px 0 0;

          .statistic-block {
            margin: 0 0 36px;
            padding: 31px 30px 40px 35px;

            .statistic-block__title {
              margin: 0 0 14px;
            }

            .statistic-block__info {
              flex-direction: column;
              align-items: flex-start;

              .info-number {
                margin: 0 0 2px;
                font-size: 59px;
                line-height: 59px;
              }

              .info-text {
                font-size: 18px;
              }
            }
          }
        }
      }
    }
  }
}
// ================================== IMPACT HIGHLIGHTS CHAIN SECTION END =========================== //

