  position: absolute;
  top: 5px;
  right: 5px;
  bottom: auto;
  left: auto;
  width: $size;
  height: $size;
  line-height: $size;
  font-size: 1.5em;
  text-align: center;
  color: #fff;
  z-index: 1;
  background: rgba(255, 255, 255, 0);
  border-radius: 50%;
  @include prefix( "transition", ".4s color, .4s background ease-in-out" );
}

.instagram-block__link:hover .instagram-block__logo {
  -webkit-animation: icon-rotate .4s linear;
  animation: icon-rotate .4s linear;
  background: rgba(255, 255, 255, .8);
  color: #000;
}

.instagram-block--info .instagram-block__bg {
