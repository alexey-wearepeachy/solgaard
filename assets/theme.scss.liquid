/*============ New Transatlantic LP Hero Section ==================*/
#new-lp--hero-section {
  position: relative;
  padding: 124px 0 146px 0;

  @media (min-width: 769px) {
    padding: 138px 0 64px 0;
  }

  .hero-section__video-bg {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    overflow: hidden;
    z-index: 1;

    video {
      position: relative;
      top: 50%;
      left: 50%;
      transform: translateX(-50%) translateY(-50%);
      min-height: 100%;
      min-width: 100%;
      height: auto;
      width: auto;
      max-width: none;
      object-fit: cover;
      z-index: inherit;
    }

    .video-bg__desk {
      display: none;

      @media (min-width: 769px) {
        display: block;
      }
    }

    .video-bg__mob {
      display: block;

      @media (min-width: 769px) {
        display: none;
      }
    }
  }

  .hero-section__container {
    position: relative;
    z-index: 2;

    max-width: 1440px;
    width: 100%;
    margin: 0 auto;

    .hero-section__wrapper {
      display: flex;
      align-items: center;
      justify-content: center;
      flex-direction: column;

      padding: 0 23px;

      @media (min-width: 400px) {
        padding: 0 33px;
      }

      @media (min-width: 769px) {
        flex-direction: row;
      }

      .hero__info-block {
        max-width: 500px;
        margin: 0;

        @media (min-width: 769px) {
          max-width: 375px;
          margin: 0 25px 0 0;
        }

        @media (min-width: 1000px) {
          max-width: 435px;
        }

        .info-block__title {
          max-width: 100%;
          margin: 0 0 12px 0;

          font-weight: 700;
          font-size: 29px;
          line-height: 35px;
          letter-spacing: 0.2em;
          text-align: center;

          @media (min-width: 390px) {
            font-size: 34px;
            line-height: 48px;
          }

          @media (min-width: 495px) {
            margin: 0 0 22px 0;

            font-size: 44px;
            line-height: 67px;
          }

          @media (min-width: 769px) {
            max-width: 375px;
            margin: 0 0 10px 0;

            font-size: 34px;
            line-height: 50px;
            text-align: start;
          }

          @media (min-width: 1000px) {
            max-width: 435px;
            margin: 0 0 22px 0;

            font-size: 44px;
            line-height: 67px;
          }
        }

        .info-block__text {
          max-width: 100%;
          margin: 0 0 41px 0;

          font-weight: 400;
          font-size: 20px;
          line-height: 32px;
          text-align: center;

          @media (min-width: 769px) {
            max-width: 397px;
            margin: 0 0 25px 0;

            font-size: 18px;
            line-height: 27px;
            text-align: start;
          }

          @media (min-width: 1000px) {
            max-width: 397px;
            margin: 0 0 36px 0;

            font-size: 20px;
            line-height: 32px;
          }
        }
      }

      .info-block__btn {
        display: none;

        max-width: 335px;
        width: 100%;
        padding: 16px 10px;

        font-weight: 700;
        font-size: 16px;
        line-height: 26px;
        text-align: center;
        letter-spacing: 0.1em;
        text-transform: uppercase;
        //color: #FFFFFF;
        text-decoration: none;

        //background-color: #1E54E0;
        border: none;
        border-radius: 2px;
        transition: all .2s ease-in-out;

        &:hover {
          //background-color: #0a3aaf;
          //color: #FFFFFF;
        }

        &.info-block__btn-mob {
          display: block;

          margin-top: 44px;
          max-width: 648px;
        }

        @media (min-width: 769px) {
          display: block;

          &.info-block__btn-mob {
            display: none;
          }
        }

        @media (min-width: 1000px) {
          max-width: 398px;
        }
      }

      .hero__img-block {
        max-width: 648px;

        img {
          display: block;
          max-width: 100%;
          width: 100%;
          margin: 0 auto;
          object-fit: cover;
        }
      }
    }
  }
}

