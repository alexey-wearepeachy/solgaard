    margin-top: .5em;
    font-size: 5em;
  }
}

.modal__subheading {
  font-size: 2em;
}

.modal__promo-code {
  display: inline-block;
  position: relative;
  margin: 0 0 1.5em;
  padding: .75em 1.5em;
  border-radius: 6px;
  font-size: 1.4em;
  font-weight: 600;
  line-height: 1.1;
  text-decoration: none;
  color: #000;
  overflow: hidden;
  cursor: pointer;

  @include at-query ($min, $large) {
    padding-right: 0;
  }

  &:before {
    position: absolute;
    display: block;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    border: 2px dashed #ccc;
    content: "";
  }

  &:after {
    @include at-query ($min, $large) {
      display: inline-block;
      position: relative;
      margin: -.75em 0 -.75em 1.5em;
      padding: .75em 1em;
      background: #000;
      color: #fff;
      content: "Copy";
    }
  }

  &:active,
  &:focus,
  &:hover {
    background: #f2f2f2;
    color: #000;

    &:before {
      border: 2px dashed #aaa;
    }
  }
