  .modal__right {
    width: 50%;
  }

  .modal__left {
    float: left;
  }

  .modal__right {
    position: absolute;
    float: none;
    top: 0;
    right: 0;
    bottom: 0;
    left: 50%;
  }
}

.caret {
  position: relative;
  display: block;
  width: 100%;
  height: 100%;
}

.caret__bar {
  display: block;
  position: absolute;
  width: 10px;
  height: 2px;
  top: 50%;
  left: 50%;
  -webkit-transform: translateX(-50%)translateY(-50%);
  transform: translateX(-50%)translateY(-50%);
  -webkit-transform-origin: 50% 50%;
  transform-origin: 50% 50%;
  content: "";
  background-color: #000;
  @include transition( .2s width ease-in-out );

  &:nth-of-type(2){
    transform: translateX(-50%)translateY(-50%)rotate(90deg);
  }
}

.caret.in,
.caret[aria-expanded="true"] {
  .caret__bar:nth-of-type(2) {
    width: 2px;
  }
}

