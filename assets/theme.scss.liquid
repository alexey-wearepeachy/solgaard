  @include transition( .15s opacity ease-in-out );
  opacity: 0;
}

.team-member__overlay {
  background-color: $colorBlack;
}

.team-member__content {
  @include transform( translateY(-50%) );
  color: $colorWhite;
  top: 50%;
  right: 15px;
  bottom: auto;
  left: 15px;
}

.team-member__social {
  margin: 0;

  .social-icons__list {
    display: inline-block;
  }

  a {
    color: $colorWhite;
  }
}

.team-member__image {
  @include transition( .15s transform ease-in-out );
  @include transform( scale(1) );
  background: {
    color: #aaa;
    size: cover;
    position: center;
  }
}

.team-member__item:hover .team-member__image {
  @include transform( scale(1.05) );
}

.team-member__item:hover .team-member__overlay,
.team-member__item:hover .team-member__content {
  opacity: .9;
}

.team-member__name {
  margin: 20px 10px 5px;
  font-weight: 600;
}

.team-member__title {
  margin-bottom: 0;
  font-size: .9em;
}



// Blog Press
.press-kit__item {
  background: $colorWhite;
  border-radius: 3px;
  box-shadow: 0 0 10px rgba(200,200,200,0.4);
  text-align: center;
  overflow: hidden;
}

.press-kit__link {
  display: block;
  padding: $gutter;
  text-decoration: none;

  @include at-query ($min, $large) {
    padding: 50px;
  }
}

.press-kit__image {
  @include transition( .15s transform ease-in-out );
  @include transform( scale(1) );
  width: 200px;
  max-width: 100%;
  margin: 0 auto;
}

.press-kit__link:hover .press-kit__image {
  @include transform( scale(1.05) );
}

.press-kit__link--disabled {
  cursor: not-allowed;
  opacity: .2;

  * {
    pointer-events: none;
  }
}

.press-kit__heading {
  margin-top: $gutter;
  margin-bottom: 0;

  @include at-query ($min, $large) {
    margin-top: $gutter;
  }
}



